{
    "python.languageServer": "Pylance",
    "[python]": {
        "editor.rulers": [
            {
                "column": 88,
                "color": "#764462"
            }
        ]
    },
    "blender.addon.logLevel": "info",
    "blender.environmentVariables": {
        "BLENDER_USER_RESOURCES": "${workspaceFolder}/.cache/",
        "PYTHONNOUSERSITE": 1
    },
}
