{
    "python.analysis.extraPaths": [
        "external/vggt",
        "external/vggt/vggt/dependency"
    ],
    "autoHide.autoHidePanel": false,
    "blender.addon.logLevel": "debug",
    "blender.addon.loadDirectory": "auto",
    "blender.addon.moduleName": "auto",
    "blender.addon.sourceDirectory": "auto",

    "workspaceKeybindings.inBlenderDevelopmentMode.enabled": true,
    "terminal.integrated.commandsToSkipShell": [
        "blender.start",
        "blender.stop",
        "blender.reloadAddons"
    ],
    "python.analysis.diagnosticSeverityOverrides": {
        "reportInvalidTypeForm": "none"
    }
}